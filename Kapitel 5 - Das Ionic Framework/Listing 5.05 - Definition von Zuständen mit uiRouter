meineApp.config([
  "$urlRouterProvider",
  "$stateProvider",
  function ($urlRouterProvider, $stateProvider) {
    // Standardroute definieren
    $urlRouterProvider.otherwise("/start");
    // States definieren
    $stateProvider
      .state("start", {
        url: "/start",
        template: "<p>Test</p>"
      });
}]);
